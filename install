#!/bin/bash
#
# install - Instalador para os programas da Receita Federal (IRPF) no Ubuntu
#
#    Copyright (C) 2012 Rodrigo Silva (MestreLion) <linux@rodrigosilva.com>
#
#    This program is free software: you can redistribute it and/or modify
#    it under the terms of the GNU General Public License as published by
#    the Free Software Foundation, either version 3 of the License, or
#    (at your option) any later version.
#
#    This program is distributed in the hope that it will be useful,
#    but WITHOUT ANY WARRANTY; without even the implied warranty of
#    MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the
#    GNU General Public License for more details.
#
#    You should have received a copy of the GNU General Public License
#    along with this program. See <http://www.gnu.org/licenses/gpl.html>
#
# Possui as seguintes características (e vantagens sobre o instalador oficial):
# - Utiliza arquivos originais da receita, que o usuário baixa do site oficial
# - Permite instalar múltiplos programas de uma vez (vários anos)
# - Permite instalação apenas para o usuário, sem necessitar de senha de root
# - Instala também o receitanet, se ele estiver disponivel na pasta
# - Cria todos os ícones de menu que o original cria
# - Instalação limpa e correta, nas pastas certas, sem criar arquivos desneces-
#     sários, e uniforme para todos os anos
# - Desisntalação igualmente limpa, sem deixar sujeira
# - Permite desinstalar apenas 1 programa (install <ano>) ou todos (purge), que
#     que também desistala o receitanet e o menu principal
# - Testado em todos os IRPF de 2008 até 2013

# Para instalar:
# - Vá no site da receita e baixe a versão ZIP do IRPF dos anos desejados
# - Baixe também o Receitanet, versão .DEB
# - Salve todos os arquivos na mesma pasta do instalador (este arquivo)
# - Configure as linhas abaixo com os parametros desejados.
#    Por padrão a instalalação é feita apenas para o usuário atual, na pasta
#    ~/.local/opt/ProgramasRFB/IRPF20XX, sem precisar de privilégios administra-
#    tivos (senha de root) e sem alterar nada no sistema ou em outros usuários
#    Para instalar para todos os usuários, mude a linha "system" de 0 para 1
# - Rode o programa a partir de qualquer pasta: ./install
# - Todos os ZIPs disponiveis (e o Receitanet tambem) serão instalados

# Para desinstalar apenas um programa de um ano especifico:
# - Mude a linha "uninstall" de 0 para 1
# - Rode o programa passando o ano desejado: ./install 2011

# Para desinstalar tudo (todos os anos, o Receitanet e o menu)
# - Mude a linha "purge" de 0 para 1
# - Rode o programa: ./install


# Root dir
root="ProgramasRFB"

# Receitanet
receitanet="Receitanet"

# install prefix for system-wide install
sysprefix=/opt

# install prefix for user install
userprefix="$HOME/.local/opt"

# install type: 1 for system-wide, 0 for user
system=0

# Uninstall flags
uninstall=0
purge=0

# -------------------------------------------------------------

if (( system )) ; then
	[[ "$prefix" ]] || prefix=$sysprefix
	sudo=sudo
	label="system"
else
	[[ "$prefix" ]] || prefix=$userprefix
	sudo=env
	label="user"
fi

rootdir=${prefix}/${root}
receitanetdir=${rootdir}/${receitanet}

decode(){
    # Generated by dataembed
    awk -v f="$1" 'p&&/^_EOF_$/{exit}p{print;next}e&&/^:<<"_EOF_"$/{getline
    if($0==f)p=1}/^exit;:$/{e=1}' "$(readlink -f "$0")" | base64 -d
}


# Desisntalacao ---------------------------------------------

purge(){
	for folder in "$rootdir"/IRPF*; do
		uninstall "$(basename "$folder")"
	done

	# Receitanet - atalhos
	xdg-desktop-menu uninstall --noupdate  \
	RFB-receitanet.desktop                 \
	RFB-receitanet-ajuda.desktop

	# Receitanet - icones
	xdg-icon-resource uninstall --noupdate --size 32 RFB-receitanet
	xdg-icon-resource uninstall --noupdate --size 32 RFB-ajuda

	# Limpa o registro Java
	java -jar "${receitanetdir}/lib/util-instalador.jar" -limpar "${label}Root" br/gov/serpro/receitanet/suite
	java -jar "${receitanetdir}/lib/util-instalador.jar" -limpar "${label}Root" br/gov/serpro/receitanet

	# Receitanet - diretorio
	rm -rf "${rootdir}/Receitanet"

	echo "Receitanet desinstalado"

	# Icone principal
	xdg-icon-resource uninstall --noupdate --size 32 RFB.png RFB-menu

	# Diretorio raiz
	rm -rf "${rootdir}"
}
uninstall(){
	local irpf="$1"

	# Desinstala atalhos
	xdg-desktop-menu uninstall --noupdate \
	RFB-${irpf,,}_program.desktop         \
	RFB-${irpf,,}_help.desktop            \
	RFB-${irpf,,}_leiame.desktop

	# Desinstala icones
	xdg-icon-resource uninstall --noupdate --size 32 "RFB-${irpf}"

	# Remove o diretorio
	rm -rf "${rootdir}/${irpf}"

	echo "${irpf} desinstalado"
}

if ((purge)); then
	purge
	xdg-icon-resource forceupdate
	xdg-desktop-menu forceupdate
	exit
else
	((uninstall)) && {
		uninstall "${irpf}"
		xdg-icon-resource forceupdate
		xdg-desktop-menu forceupdate
		exit
	}
fi


mydir=$(dirname "$(readlink -f "$0")")
cd "$mydir"

# Instalacao da pasta raiz ---------------------------

mkdir -p "$rootdir"

cat > RFB-ProgramasRFB.directory <<EOF
[Desktop Entry]
Version=1.0
Type=Directory
Name=Imposto de Renda
Comment=Receita Federal do Brasil
Icon=RFB-menu
EOF

decode "RFB.png" > "${rootdir}/RFB.png"

xdg-icon-resource install --noupdate --size 32 "${rootdir}/RFB.png" RFB-menu

# Instalacao Receitanet ------------------------------

if [[ -f receitanet-1.03.deb ]]; then

dpkg-deb --fsys-tarfile receitanet-1.03.deb |
tar -x --strip-components=3 --directory="$rootdir" './opt/Programas RFB/Receitanet/'

# Grava no registro Java
java -jar "${receitanetdir}/lib/util-instalador.jar" -escrever "${label}Root" br/gov/serpro/receitanet/suite Path "${receitanetdir}" Origem A Tipo suite Versao 201104 Instalacao D
java -jar "${receitanetdir}/lib/util-instalador.jar" -escrever "${label}Root" br/gov/serpro/receitanet       Path "${receitanetdir}" Origem A Tipo suite Versao 201104 Instalacao D


cat > RFB-ProgramasRFB-Receitanet.directory <<EOF
[Desktop Entry]
Version=1.0
Type=Directory
Name=Receitanet
Icon=RFB-receitanet
EOF

cat > RFB-receitanet.desktop <<EOF
[Desktop Entry]
Version=1.0
Type=Application
Terminal=false
Exec=java -jar '${receitanetdir}/receitanet.jar'
Name=Receitanet 1.03
Comment=Executa Receitanet 1.03
Icon=RFB-receitanet
EOF

cat > RFB-receitanet-ajuda.desktop <<EOF
[Desktop Entry]
Version=1.0
Type=Application
Terminal=false
Exec=java -jar '${receitanetdir}/receitanet.jar' -ajuda
Name=Ajuda do Receitanet
Comment=Ajuda do Receitanet
Icon=RFB-ajuda
EOF

xdg-icon-resource install --noupdate --size 32 "${receitanetdir}/imagens/Receitanet.xpm" RFB-receitanet
xdg-icon-resource install --noupdate --size 32 "${receitanetdir}/imagens/Ajuda.xpm"      RFB-ajuda
xdg-desktop-menu  install --noupdate  \
RFB-ProgramasRFB.directory            \
RFB-ProgramasRFB-Receitanet.directory \
RFB-receitanet.desktop                \
RFB-receitanet-ajuda.desktop

echo "Receitanet instalado"

else
	"Receitanet não encontrado"
fi

# Instalacao do programas  ------------------------

for zip in *.zip; do
	irpf="${zip:0:8}"
	year="${zip:4:4}"
	irpfdir=${rootdir}/${irpf^^}

	mkdir -p "${irpfdir}"

	# Alguns zips nao estao no formato padrao, por isso o diretorio temporario
	unzip -q "$zip" -d "temp"
	tar -c temp | tar -x --strip-components=2 --directory="${irpfdir}"
	rm -rf temp

	cat > RFB-ProgramasRFB-${irpf^^}.directory <<-EOF
	[Desktop Entry]
	Version=1.0
	Type=Directory
	Name=${irpf^^}
	Comment=Declaração de Ajuste Anual ${year}
	Icon=RFB-menu
	EOF

	cat > RFB-${irpf,,}_program.desktop <<-EOF
	[Desktop Entry]
	Version=1.0
	Type=Application
	Name=${irpf^^} - Declaração de Ajuste Anual
	Comment=${irpf^^}
	Exec=java -Xms128M -Xmx512M -jar '${irpfdir}/irpf.jar'
	Icon=RFB-menu
	EOF

	cat > RFB-${irpf,,}_help.desktop <<-EOF
	[Desktop Entry]
	Version=1.0
	Type=Application
	Name=Ajuda do ${irpf^^}
	Exec=java -jar '${irpfdir}/irpf.jar' -h
	Icon=RFB-ajuda
	EOF

	cat > RFB-${irpf,,}_leiame.desktop <<-EOF
	[Desktop Entry]
	Version=1.0
	Type=Application
	Name=Leia-me do ${irpf^^}
	Exec=xdg-open '${irpfdir}/Leia_me.htm'
	Icon=RFB-ajuda
	EOF

	# Instala menus
	xdg-desktop-menu install --noupdate   \
	RFB-ProgramasRFB.directory            \
	RFB-ProgramasRFB-${irpf^^}.directory  \
	RFB-${irpf,,}_program.desktop         \
	RFB-${irpf,,}_help.desktop            \
	RFB-${irpf,,}_leiame.desktop

	# Remove lixo e complementa a instalacao
	rm -rf "${irpfdir}"/[Ee]xec*
	rm -rf "${irpfdir}"/*.exe
	[[ -f "${irpfdir}/online.png"  ]] || decode "online.png"  > "${irpfdir}/online.png"
	[[ -f "${irpfdir}/offline.png" ]] || decode "offline.png" > "${irpfdir}/offline.png"

	echo "${irpf^^} instalado com sucesso"
done

# Aplica todas as atualizacoes
xdg-icon-resource forceupdate
xdg-desktop-menu  forceupdate
rm -rf *.{directory,desktop,png}


# Generated by dataembed - DO NOT EDIT AFTER THIS LINE! ---------------
exit;:
:<<"_EOF_"
RFB.png
